{
  "storage":{
    "rootDirectory": "/var/lib/registry"
  },
  "http":{
    "address":"0.0.0.0",
    "port":"5000",
    "compat": ["docker2s2"]
  },
  "log":{
    "level":"debug"
  },
  "extensions": {
    "search": {
      "enable": true,
      "cve": {
          "updateInterval": "2h"
      }
    },
    "ui": {
      "enable": true
    },
    "mgmt": {
      "enable": true
    }
  }
}